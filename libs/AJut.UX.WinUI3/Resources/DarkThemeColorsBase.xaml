<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="using:System">

    <!-- ================[ Highlights ]====================== -->
    <Color x:Key="AJut_Color_PrimaryHighlight">#0D83D8</Color>
    <Color x:Key="AJut_Color_PrimaryHighlightDark">#074470</Color>
    <Color x:Key="AJut_Color_PrimaryHighlightBright">#119CFF</Color>
    <Color x:Key="AJut_Color_PrimaryHighlightDesat">#DD3C6A89</Color>
    <Color x:Key="AJut_Color_SecondaryHighlight">#D34600</Color>
    <Color x:Key="AJut_Color_SecondaryHighlightDark">#631F00</Color>
    <Color x:Key="AJut_Color_SecondaryHighlightBright">#EA4A00</Color>
    <Color x:Key="AJut_Color_SecondaryHighlightDesat">#B76138</Color>
    
    <SolidColorBrush x:Key="AJut_Brush_PrimaryHighlight" Color="{ThemeResource AJut_Color_PrimaryHighlight}"/>
    <SolidColorBrush x:Key="AJut_Brush_PrimaryHighlightDark" Color="{ThemeResource AJut_Color_PrimaryHighlightDark}"/>
    <SolidColorBrush x:Key="AJut_Brush_PrimaryHighlightBright" Color="{ThemeResource AJut_Color_PrimaryHighlightBright}"/>
    <SolidColorBrush x:Key="AJut_Brush_PrimaryHighlightDesat" Color="{ThemeResource AJut_Color_PrimaryHighlightDesat}"/>
    <SolidColorBrush x:Key="AJut_Brush_SecondaryHighlight"  Color="{ThemeResource AJut_Color_SecondaryHighlight}"/>
    <SolidColorBrush x:Key="AJut_Brush_SecondaryHighlightDark"  Color="{ThemeResource AJut_Color_SecondaryHighlightDark}"/>
    <SolidColorBrush x:Key="AJut_Brush_SecondaryHighlightBright" Color="{ThemeResource AJut_Color_SecondaryHighlightBright}"/>
    <SolidColorBrush x:Key="AJut_Brush_SecondaryHighlightDesat" Color="{ThemeResource AJut_Color_SecondaryHighlightDesat}"/>

    <!-- ================[ Specialty Border Accents ]====================== -->
    <Color x:Key="AJut_Color_RimLighting">#7FFF</Color>
    <Color x:Key="AJut_Color_Shadow">#8000</Color>

    <SolidColorBrush x:Key="AJut_Brush_Rimlighting" Color="{ThemeResource AJut_Color_RimLighting}"/>
    <SolidColorBrush x:Key="AJut_Brush_Shadow" Color="{ThemeResource AJut_Color_Shadow}"/>

    <Color x:Key="AJut_Color_MouseInteractionHighlight">#7F6E44</Color>
    <SolidColorBrush x:Key="AJut_Brush_MouseInteractionHighlight" Color="{ThemeResource AJut_Color_MouseInteractionHighlight}"/>

    <!-- ================[ Text & Glyphs ]====================== -->
    <Color x:Key="AJut_Color_ImportantText">#FFF</Color>
    <Color x:Key="AJut_Color_NormalText">#BBB</Color>
    <Color x:Key="AJut_Color_HighlightText">#CCC</Color>
    <Color x:Key="AJut_Color_UnderstatedText">#999</Color>
    <Color x:Key="AJut_Color_InteractiveText">#FFF</Color>
    <Color x:Key="AJut_Color_InteractiveTextUnfocused">#BBB</Color>
    <Color x:Key="AJut_Color_HighContrastText">#CCC</Color>
    <Color x:Key="AJut_Color_SymbolForeground">#CCC</Color>
    <Color x:Key="AJut_Color_SymbolForegroundPressed">#DDD</Color>
    <Color x:Key="AJut_Color_SymbolForegroundHighlight">#FFF</Color>

    <SolidColorBrush x:Key="AJut_Brush_ImportantText" Color="{ThemeResource AJut_Color_ImportantText}" />
    <SolidColorBrush x:Key="AJut_Brush_NormalText" Color="{ThemeResource AJut_Color_NormalText}" />
    <SolidColorBrush x:Key="AJut_Brush_HighlightText" Color="{ThemeResource AJut_Color_HighlightText}" />
    <SolidColorBrush x:Key="AJut_Brush_UnderstatedText" Color="{ThemeResource AJut_Color_UnderstatedText}" />
    <SolidColorBrush x:Key="AJut_Brush_InteractiveText" Color="{ThemeResource AJut_Color_InteractiveText}" />
    <SolidColorBrush x:Key="AJut_Brush_InteractiveTextUnfocused" Color="{ThemeResource AJut_Color_InteractiveTextUnfocused}" />
    <SolidColorBrush x:Key="AJut_Brush_HighContrastText" Color="{ThemeResource AJut_Color_HighContrastText}" />
    <SolidColorBrush x:Key="AJut_Brush_SymbolForeground" Color="{ThemeResource AJut_Color_SymbolForeground}" />
    <SolidColorBrush x:Key="AJut_Brush_SymbolForegroundPressed" Color="{ThemeResource AJut_Color_SymbolForegroundPressed}" />
    <SolidColorBrush x:Key="AJut_Brush_SymbolForegroundHighlight" Color="{ThemeResource AJut_Color_SymbolForegroundHighlight}" />

    <SolidColorBrush x:Key="AJut_Brush_HyperLinkNormal" Color="{ThemeResource AJut_Color_PrimaryHighlight}"/>
    <SolidColorBrush x:Key="AJut_Brush_HyperLinkHover" Color="{ThemeResource AJut_Color_SecondaryHighlight}"/>

    <!-- ================[ Lists ]====================== -->
    <Color x:Key="AJut_Color_ListBackground">#36393A</Color>
    <Color x:Key="AJut_Color_ListBorder">#000</Color>
    <Color x:Key="AJut_Color_ListSelection">#0864A5</Color>
    <Color x:Key="AJut_Color_ListSelectionBackground">#19119CFF</Color>
    <Color x:Key="AJut_Color_TreeSelection">#0864A5</Color>
    <StaticResource x:Key="AJut_Color_ListItemHover" ResourceKey="AJut_Color_PrimaryHighlight"/>

    <SolidColorBrush x:Key="AJut_Brush_ListBackground" Color="{ThemeResource AJut_Color_ListBackground}" />
    <SolidColorBrush x:Key="AJut_Brush_ListBorder" Color="{ThemeResource AJut_Color_ListBorder}" />
    <SolidColorBrush x:Key="AJut_Brush_ListSelection" Color="{ThemeResource AJut_Color_ListSelection}" />
    <SolidColorBrush x:Key="AJut_Brush_ListSelectionBackground" Color="{ThemeResource AJut_Color_ListSelectionBackground}" />
    <SolidColorBrush x:Key="AJut_Brush_TreeSelection" Color="{ThemeResource AJut_Color_TreeSelection}" />
    <SolidColorBrush x:Key="AJut_Brush_ListItemHover" Color="{ThemeResource AJut_Color_ListItemHover}" />

    <!-- ================[ Panels & Borders ]====================== -->
    <Color x:Key="AJut_Color_BorderCommon">#555</Color>
    <Color x:Key="AJut_Color_BorderCommonTransparent">#0555</Color>
    <SolidColorBrush x:Key="AJut_Brush_BorderCommon" Color="{ThemeResource AJut_Color_BorderCommon}"/>
    <SolidColorBrush x:Key="AJut_Brush_BorderCommonHover" Color="#888"/>
    <SolidColorBrush x:Key="AJut_Brush_BorderCommonPress" Color="#AAA"/>
    <SolidColorBrush x:Key="AJut_Brush_BorderShadow" Color="{ThemeResource AJut_Color_Shadow}"/>

    <SolidColorBrush x:Key="AJut_Brush_RootPanelBackground" Color="#2C2C30"/>
    <SolidColorBrush x:Key="AJut_Brush_PanelBackground" Color="#18181D"/>

    <Color x:Key="AJut_Color_InnerPanelBackground">#252528</Color>
    <Color x:Key="AJut_Color_InnerPanelBackgroundHover">#2B2B3C</Color>
    <SolidColorBrush x:Key="AJut_Brush_InnerPanelBackground" Color="{ThemeResource AJut_Color_InnerPanelBackground}"/>
    <SolidColorBrush x:Key="AJut_Brush_PanelBorder" Color="#111"/>

    <SolidColorBrush x:Key="AJut_Brush_PopupBackground" Color="#55555B"/>

    <SolidColorBrush x:Key="AJut_Brush_ControlBackground" Color="#111117"/>
    <LinearGradientBrush x:Key="AJut_Brush_PopOutControlBackground"  StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0.00" Color="#585865" />
        <GradientStop Offset="0.10" Color="#353540" />
        <GradientStop Offset="0.90" Color="#222228" />
        <GradientStop Offset="1.00" Color="#000008" />
    </LinearGradientBrush>
    <SolidColorBrush x:Key="AJut_Brush_ControlBackgroundHover" Color="#222225"/>
    <SolidColorBrush x:Key="AJut_Brush_ButtonBackgroundPressed" Color="#050510"/>
    <SolidColorBrush x:Key="AJut_Brush_TextBoxBackground" Color="#444"/>
    <SolidColorBrush x:Key="AJut_Brush_TextBoxBorder" Color="#222"/>

    <SolidColorBrush x:Key="AJut_Brush_SimpleHighContrastRootBackground" Color="#131317"/>

    <sys:Double x:Key="AJut_Double_StandardShadowOpacity">0.6</sys:Double>
    <sys:Double x:Key="AJut_Double_StandardDisabledOpacity">0.4</sys:Double>

    <!-- ===================[ Window / Titlebar / Navigation ]========================= -->
    <Color x:Key="AJut_Color_ChromeButtonBackground">Transparent</Color>
    <Color x:Key="AJut_Color_ChromeButtonForeground">#BBB</Color>
    
    <Color x:Key="AJut_Color_ChromeButtonHoverBackground">#000</Color>
    <Color x:Key="AJut_Color_ChromeButtonHoverForeground">#CCC</Color>
    
    <StaticResource x:Key="AJut_Color_ChromeButtonPressedBackground" ResourceKey="AJut_Color_PrimaryHighlight"/>
    <Color x:Key="AJut_Color_ChromeButtonPressedForeground">#202020</Color>

    <Color x:Key="AJut_Color_ChromeButtonInactiveBackground">Transparent</Color>
    <Color x:Key="AJut_Color_ChromeButtonInactiveForeground">#444</Color>
    <Color x:Key="AJut_Color_ChromeCloseButtonHoverBackground">#E81123</Color>
    <Color x:Key="AJut_Color_ChromeCloseButtonHoverForeground">#FFF</Color>
    <Color x:Key="AJut_Color_ChromeCloseButtonPressedBackground">#E81123</Color>
    <Color x:Key="AJut_Color_ChromeCloseButtonPressedForeground">#FFF</Color>
    <Color x:Key="AJut_Color_ChromeCloseButtonComplement">#892630</Color>

    <SolidColorBrush x:Key="AJut_Brush_ChromeCloseButtonHighlight" Color="{ThemeResource AJut_Color_ChromeCloseButtonHighlight}"/>
    <SolidColorBrush x:Key="AJut_Brush_ChromeCloseButtonComplement" Color="{ThemeResource AJut_Color_ChromeCloseButtonComplement}"/>

    <SolidColorBrush x:Key="AJut_Brush_WindowOuterBorder" Color="{ThemeResource AJut_Color_PrimaryHighlightBright}"/>
    <SolidColorBrush x:Key="AJut_Brush_SecondaryWindowOuterBorder" Color="#30303C"/>
    <SolidColorBrush x:Key="AJut_Brush_WindowGlowBorder" Color="{ThemeResource AJut_Color_PrimaryHighlightBright}"/>
    <SolidColorBrush x:Key="AJut_Brush_WindowGlowBorderInactive" Color="{ThemeResource AJut_Color_PrimaryHighlightDark}"/>

    <Color x:Key="AJut_Color_TitleBarBackground">#202020</Color>
    <Color x:Key="AJut_Color_TitleBarBackgroundInactive">#2C2C2C</Color>
    
    <SolidColorBrush x:Key="AJut_Brush_TitleBarBackground" Color="{ThemeResource AJut_Color_TitleBarBackground}"/>
    <SolidColorBrush x:Key="AJut_Brush_TitleBarBackgroundInactive" Color="{ThemeResource AJut_Color_TitleBarBackgroundInactive}"/>
    
    <SolidColorBrush x:Key="AJut_Brush_TitleBarForeground" Color="#CCC" />
    <SolidColorBrush x:Key="AJut_Brush_TitleBarForegroundDesat" Color="#AAA" />
    <SolidColorBrush x:Key="AJut_Brush_TitleBarForegroundImportant" Color="#FFF" />
    <SolidColorBrush x:Key="AJut_Brush_TitleBarGlyphHighlight" Color="#CCC" />

    <SolidColorBrush x:Key="AJut_Brush_DrawerBackground" Color="#333"/>
    <SolidColorBrush x:Key="AJut_Brush_DrawerForeground" Color="#CCC" />
    <SolidColorBrush x:Key="AJut_Brush_DrawerForegroundImportant" Color="#000" />

    <!-- ===================[ Misc ]========================= -->
    <SolidColorBrush x:Key="AJut_Brush_BadStuff" Color="#A31B22"/>
    <SolidColorBrush x:Key="AJut_Brush_TrackBackground" Color="#444"/>
    <SolidColorBrush x:Key="AJut_Brush_ToolTipBackground" Color="#202020"/>
    <SolidColorBrush x:Key="AJut_Brush_BannerBackground" Color="#111117"/>
    <SolidColorBrush x:Key="AJut_Brush_BannerForeground" Color="#BBB"/>
    <SolidColorBrush x:Key="AJut_Brush_OverlayBlockoutBackground" Color="#D000"/>

    <SolidColorBrush x:Key="AJut_Brush_GroupBorder" Color="#636363"/>

</ResourceDictionary>