<UserControl x:Class="TheAJutShowRoom.UI.Pages.DockExampleControls.CommandSender"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TheAJutShowRoom.UI.Pages.DockExampleControls"
             xmlns:pages="clr-namespace:TheAJutShowRoom.UI.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <TextBlock Text="This button will send a command from here"/>
        <Button Command="{x:Static pages:DockingFrameworkOverviewPage.ShowPopupCommand}" Content="Click to show popup"/>
    </StackPanel>
</UserControl>
